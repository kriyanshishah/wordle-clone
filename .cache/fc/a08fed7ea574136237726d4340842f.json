\r\n    margin: 0;\r\n  }\r\n  h1 {\r\n    font-size: 1.2em;\r\n    padding: 15px 0 0px 0;\r\n    margin: 0 0 20px 0;\r\n    color: #333;\r\n  }\r\n  \r\n  .heading{\r\n    text-align: center;\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  \r\n  /* .heading > div{\r\n    display: block;\r\n    width: 50px;\r\n    height: 50px;\r\n    border: 1px solid #bbb;\r\n    background-color: #5ac85a;\r\n    color: white;\r\n    margin: 6px;\r\n    text-align: center;\r\n    line-height: 50px;\r\n    text-transform: uppercase;\r\n    font-weight: bold;\r\n    font-size: 1.7em;\r\n  } */\r\n  \r\n  body{\r\n    background: white;\r\n    font-family: 'Montserrat', sans-serif;\r\n  }\r\n  \r\n  .word {\r\n      display: inline-block;\r\n      font-size: 35px;\r\n      line-height: 50px;\r\n      cursor: pointer;\r\n      margin: 10px;\r\n      width: 50px;\r\n      height: 50px;\r\n  /*     border-radius: 50%; */\r\n      text-align: center;\r\n      position: relative;\r\n      text-decoration: none;\r\n      z-index: 1;\r\n      background: #fff;\r\n    color: #5ac85a;\r\n  }\r\n  \r\n  .yellow{\r\n    background: #e2cc68;\r\n  }\r\n  .word:after {\r\n      pointer-events: none;\r\n      position: absolute;\r\n      width: 100%;\r\n      height: 100%;\r\n  /*     border-radius: 50%; */\r\n      content:'';\r\n      -webkit-box-sizing: content-box;\r\n      -moz-box-sizing: content-box;\r\n      box-sizing: content-box;\r\n  }\r\n  .word:before {\r\n      /* speak: h; */\r\n      font-style: normal;\r\n      font-weight: normal;\r\n      font-variant: normal;\r\n      text-transform: none;\r\n      display: block;\r\n      -webkit-font-smoothing: antialiased;\r\n  }\r\n  \r\n  .word.effect-2 {\r\n      color: #fff;\r\n      box-shadow: 0 0 0 3px #fff;\r\n      -webkit-transition: color 0.3s;\r\n      -moz-transition: color 0.3s;\r\n      transition: color 0.3s;\r\n  }\r\n  .word.effect-2:after {\r\n      top: -2px;\r\n      left: -2px;\r\n      padding: 2px;\r\n      z-index: -1;\r\n      background: #5ac85a;\r\n      -webkit-transition: -webkit-transform 0.2s, opacity 0.2s;\r\n      -moz-transition: -moz-transform 0.2s, opacity 0.2s;\r\n      transition: transform 0.2s, opacity 0.2s;\r\n  }\r\n  \r\n  .yellow.effect-2:after {\r\n    background: #e2cc68;\r\n    color: #fff;\r\n  }\r\n  \r\n  .grey.effect-2:after {\r\n    background: #9f9e9e;\r\n    color: #fff;\r\n  }\r\n  \r\n  .word.effect-2.sub-b:hover:after {\r\n      -webkit-transform: scale(0);\r\n      -moz-transform: scale(0);\r\n      -ms-transform: scale(0);\r\n      transform: scale(0);\r\n      opacity: 0;\r\n      -webkit-transition: -webkit-transform 0.4s, opacity 0.2s;\r\n      -moz-transition: -moz-transform 0.4s, opacity 0.2s;\r\n      transition: transform 0.4s, opacity 0.2s;\r\n  }\r\n  .word.effect-2.sub-b:hover, .word.effect-2.sub-b:hover i {\r\n      color: #5ac85a;\r\n  }\r\n  \r\n  .yellow.effect-2.sub-b:hover, .yellow.effect-2.sub-b:hover i {\r\n      color: #e2cc68;\r\n    background: white;\r\n  }\r\n  \r\n  .grey.effect-2.sub-b:hover, .grey.effect-2.sub-b:hover i {\r\n      color: #9f9e9e;\r\n    background: #fff;\r\n  }\r\n  .row{\r\n    text-align: center;\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .row > div {\r\n    display: block;\r\n    width: 50px;\r\n    height: 50px;\r\n    border: 1px solid #bbb;\r\n    border-radius: 10px;\r\n    margin: 6px;\r\n    text-align: center;\r\n    line-height: 50px;\r\n    text-transform: uppercase;\r\n    font-weight: bold;\r\n    font-size: 1.7em;\r\n  }\r\n  \r\n  .row > div.green {\r\n    --background: #5ac85a;\r\n    --border-color: #5ac85a;\r\n    animation: flip 0.5s ease forwards;\r\n  }\r\n  .row > div.grey {\r\n    --background: #9f9e9e;\r\n    --border-color: #9f9e9e;\r\n    animation: flip 0.5s ease forwards;\r\n  }\r\n  .row > div.yellow {\r\n    --background: #e2cc68;\r\n    --border-color: #e2cc68;\r\n    animation: flip 0.5s ease forwards;\r\n  }\r\n  \r\n  .row > div:nth-child(2) {\r\n    animation-delay: 0.2s;\r\n  }\r\n  .row > div:nth-child(3) {\r\n    animation-delay: 0.4s;\r\n  }\r\n  .row > div:nth-child(4) {\r\n    animation-delay: 0.6s;\r\n  }\r\n  .row > div:nth-child(5) {\r\n    animation-delay: 0.8s;\r\n  }\r\n  \r\n  \r\n  .row.current > div.filled{\r\n    animation: bounce 0.2s ease-in-out forwards;\r\n  }\r\n  \r\n  .keypad {\r\n    max-width: 500px;\r\n    margin: 20px auto;\r\n  }\r\n  .keypad > div {\r\n    margin: 5px;\r\n    width: 40px;\r\n    height: 50px;\r\n    background: #eee;\r\n    display: inline-block;\r\n    border-radius: 6px;\r\n    line-height: 50px;\r\n  }\r\n  \r\n  .keypad > div.green {\r\n    background: #5ac85a;\r\n    color: #fff;\r\n    transition: all 0.3s ease-in;\r\n  }\r\n  .keypad > div.yellow {\r\n    background: #e2cc68;\r\n    color: #fff;\r\n    transition: all 0.3s ease-in;\r\n  }\r\n  .keypad > div.grey {\r\n    background: #a1a1a1;\r\n    color: #fff;\r\n    transition: all 0.3s ease-in;\r\n  }\r\n  \r\n  .modal {\r\n    background: rgba(255,255,255,0.7);\r\n    position: fixed;\r\n    width: 100%;\r\n    height: 100%;\r\n    top: 0;\r\n    left: 0;\r\n  }\r\n  .modal div {\r\n    max-width: 480px;\r\n    background: #fff;\r\n    padding: 40px;\r\n    border-radius: 10px;\r\n    margin: 10% auto;\r\n    box-shadow: 2px 2px 10px rgba(0,0,0,0.3);\r\n  }\r\n  .modal .solution {\r\n    color: #ff004c;\r\n    font-weight: bold;\r\n    font-size: 0.8em;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n  }\r\n  \r\n  /* animation - of the tiles turning*/\r\n  \r\n  @keyframes flip {\r\n    0%{\r\n      transform: rotateY(0);\r\n      background: #fff;\r\n      border-color: #d3d3d3;\r\n    }\r\n  \r\n    45%{\r\n      transform: rotateY(90deg);\r\n      background: #fff;\r\n      border-color: #d3d3d3;\r\n    }\r\n  \r\n    55%{\r\n      transform: rotateY(90deg);\r\n      background-color: var(--background);\r\n      border-color: var(--border-color);\r\n      color: #eee;\r\n    }\r\n  \r\n    100%{\r\n      /* transform: rotateX(0); */\r\n      transform: rotateY(0);\r\n      background-color: var(--background);\r\n      border-color: var(--border-color);\r\n      color: #eee;\r\n    }\r\n  }\r\n  \r\n  @keyframes bounce {\r\n    0%{\r\n      transform: scale(1);\r\n      border-color: #d3d3d3;\r\n    }\r\n  \r\n    50%{\r\n      transform: scale(1.1);\r\n    }\r\n  \r\n    100%{\r\n      transform: scale(1);\r\n      border-color: #333;\r\n    }\r\n    \r\n  }"},"lineCount":286}},"error":null,"hash":"b3f2d274f2e16546c0ab68815fa2681c","cacheData":{"env":{}}}